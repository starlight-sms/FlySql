<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>A template engine for writing dynamic sql.</Description>
    <TargetFrameworks>netcoreapp2.0;net451</TargetFrameworks>
    <AssemblyName>sdmap</AssemblyName>
    <PackageId>FlySql</PackageId>
    <PackageTags>dynamic sql;ibatis</PackageTags>
    <PackageReleaseNotes>https://github.com/starlight-sms/FlySql/blob/master/ReleaseNotes.md</PackageReleaseNotes>
    <PackageLicenseUrl>https://github.com/starlight-sms/FlySql/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/starlight-sms/FlySql</RepositoryUrl>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <Version>0.12.0.5</Version>
    <AssemblyVersion>0.12.0.5</AssemblyVersion>
    <FileVersion>0.12.0.5</FileVersion>
    <Authors>sdcb</Authors>
    <PackageProjectUrl>https://github.com/starlight-sms/FlySql</PackageProjectUrl>
    <Copyright>MIT</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <Antlr4 Include="Parser/G4/SdmapLexer.g4">
      <CustomToolNamespace>FlySql.Parser.G4</CustomToolNamespace>
    </Antlr4>
    <Antlr4 Include="Parser/G4/SdmapParser.g4">
      <CustomToolNamespace>FlySql.Parser.G4</CustomToolNamespace>
    </Antlr4>
  </ItemGroup>

  <ItemGroup>
    <None Include="Emiter\Implements\Common\CodeEmiterConfig.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Antlr4.Runtime" Version="4.6.4" />
    <PackageReference Include="Antlr4.CodeGenerator" Version="4.6.4">
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
  </ItemGroup>

</Project>
